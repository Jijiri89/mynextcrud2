
generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model User {
  id Int @id @default(autoincrement())
  name String
  email String
  
}
model Country{
  id Int @id @default(autoincrement())
  name String
  code String
}
model Product{
  id Int @id @default(autoincrement())
  name String
  price Decimal @db.Decimal(10,2)
  type String
}
model Post{
  id Int @id @default(autoincrement())
  name String
  auther String
}
model Category{
  id Int @id @default(autoincrement())
  name String
  color String 
}